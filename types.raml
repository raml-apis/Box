      total_count: integer
      entries: Collaboration[]
    example:
      total_count: 1
      entries:
        - type: collaboration
          id: "791293"
          created_by:
            type: user
            id: "17738362"
            name: sean rose
            login: sean@box.com
          created_at: 2012-12-12T10:54:37-08:00
          modified_at: 2012-12-12T11:30:43-08:00
          expires_at: null
          status: accepted
          accessible_by:
            type: user
            id: "18203124"
            name: sean
            login: sean+test@box.com
          role: editor
