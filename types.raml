      id: string
      item:
        properties:
          type: string
          id: string
          sequence_id: string
          etag: string
          sha1: string
          name: string
        type: object
      assigned_to:
        properties:
          type: string
          id: string
          name: string
          login: string
        type: object
      message?: string
      completed_at?: string
      assigned_at?: string
      reminded_at?: string
      resolution_state?: string
      assigned_by?:
